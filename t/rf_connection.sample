    {
      if(CmDownstreamFrequency[i] > 0){
          document.write('<td width="20%">'+CmDownstreamChannelPowerdBmV[i]+' dBmV');
      }else{
          document.write('<td width="20%"> </td>');
			}
    }
  </script>
<tr bgcolor="#C0C0C0"><td width="20%">Signal noise ratio:
