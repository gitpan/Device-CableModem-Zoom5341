<tr bgcolor="#C0C0C0"><td width="20%">Channel Frequency:
  <script language="javascript">
    for (var i=0; i<4; i++)
    {
      if(CmDownstreamFrequency[i] > 0){
          document.write('<td width="20%">'+CmDownstreamFrequency[i]+' Hz</td>');
      }else{
          document.write('<td width="20%"> </td>');
			}
    }
  </script>
<tr bgcolor="#C0C0C0"><td width="20%">Modulation:
  <script language="javascript">
    for (var i=0; i<4; i++)
    {
      if(CmDownstreamFrequency[i] > 0){
          document.write('<td width="20%">'+CmDownstreamQam[i]);
      }else{
          document.write('<td width="20%"> </td>');
			}
    }
  </script>
<tr bgcolor="#C0C0C0"><td width="20%">Signal strength:
